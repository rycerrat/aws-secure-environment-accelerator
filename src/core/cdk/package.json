{
  "name": "@aws-accelerator/accelerator",
  "version": "1.5.3",
  "scripts": {
    "bootstrap": "pnpx cdk bootstrap",
    "deploy": "pnpx cdk deploy",
    "deploy:noapproval": "pnpx cdk deploy --require-approval never",
    "synth": "pnpx cdk synth",
    "test": "pnpx jest",
    "lint:typecheck": "pnpx tsc --noEmit",
    "lint:eslint": "pnpx eslint '{cdk,lib,src}/**/*.{js,ts}'"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/test/**/(*.)+(spec|test).ts"
    ]
  },
  "dependencies": {
    "@aws-accelerator/accelerator-runtime": "workspace:*",
    "@aws-accelerator/cdk-accelerator": "workspace:*",
    "@aws-cdk/aws-codebuild": "2.28.0",
    "@aws-cdk/aws-dynamodb": "2.28.0",
    "@aws-cdk/aws-events": "2.28.0",
    "@aws-cdk/aws-iam": "2.28.0",
    "@aws-cdk/aws-kms": "2.28.0",
    "@aws-cdk/aws-lambda": "2.28.0",
    "@aws-cdk/aws-s3": "2.28.0",
    "@aws-cdk/aws-s3-assets": "2.28.0",
    "@aws-cdk/aws-secretsmanager": "2.28.0",
    "@aws-cdk/aws-sns": "2.28.0",
    "@aws-cdk/aws-stepfunctions": "2.28.0",
    "@aws-cdk/aws-stepfunctions-tasks": "2.28.0",
    "@aws-cdk/core": "2.28.0"
  },
  "devDependencies": {
    "@types/node": "14.14.31",
    "aws-cdk": "2.28.0",
    "eslint": "7.25.0",
    "jest": "25.2.4",
    "ts-jest": "25.3.0",
    "ts-node": "9.1.1",
    "typescript": "4.2.4"
  }
}
